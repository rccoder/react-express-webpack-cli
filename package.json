  "bugs": {
    "url": "https://github.com/rccoder/react-express-webpack-cli/issues"
  },
  "homepage": "https://github.com/rccoder/react-express-webpack-cli#readme",
  "dependencies": {
    "body-parser": "^1.15.1",
    "express": "^4.13.4",
    "express-mysql-session": "^1.0.0",
    "express-session": "^1.13.0",
    "material-ui": "^0.15.0",
    "path": "^0.12.7",
    "react": "^15.0.2",
    "react-dom": "^15.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "css-loader": "^0.23.1",
    "jsx-loader": "^0.13.2",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "proxy-middleware": "^0.15.0",
    "react-hot-loader": "^1.3.0",
    "react-tap-event-plugin": "^1.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}