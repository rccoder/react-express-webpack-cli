{
  "name": "react-express-webpack-cli",
  "version": "1.0.0",
  "description": "react express webpack cli",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server-build": "babel server/src/ -d server/build/",
    "client-build": "webpack",
    "dev": "set NODE_ENV=dev && babel-node server/src/app.js",
    "build": "npm run server-build && npm run client-build",
    "start": "npm run server-build && npm run client-build && set NODE_ENV=production && node server/build/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rccoder/react-express-webpack-cli.git"
  },
  "author": "",
